{
  "crons": [
    {
      "path": "/api/cron/update-prices",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron/update-prices?force=hour",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/update-prices?force=hour4",
      "schedule": "0 0,4,8,12,16,20 * * *"
    },
    {
      "path": "/api/cron/update-prices?force=hour24",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/cleanup",
      "schedule": "5 0 * * *"
    }
  ]
}
